<% expressions.each do |name, info| %>
	<td data-delete_path="<%= delete_path %>" data-update_path="<%= update_path %>" class="<%= repeated %> show tactic <%= name %>">
	  <%= render partial: "expression_text", locals: {text: info[:text]} %>
		<%= render partial: "metadata", locals: {text: info[:text], metadata: info[:metadata]} %>
	</td>
<% end %>
